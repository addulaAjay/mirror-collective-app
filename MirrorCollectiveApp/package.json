{
  "name": "MirrorCollectiveApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --port 8083",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean:ios": "(killall -9 node watchman || true) && rm -rf node_modules ios/Pods ios/Podfile.lock && npm install && cd ios && pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd .. && npm run android",
    "build:ios:archive": "xcodebuild -workspace ios/MirrorCollectiveApp.xcworkspace -scheme MirrorCollectiveApp -configuration Release -sdk iphoneos -archivePath ios/build/MirrorCollectiveApp.xcarchive clean archive",
    "build:ios:ipa": "xcodebuild -exportArchive -archivePath ios/build/MirrorCollectiveApp.xcarchive -exportOptionsPlist ios/build/ExportOptions.plist -exportPath ios/build",
    "distribute:ios": "firebase appdistribution:distribute ios/build/MirrorCollectiveApp.ipa --app 1:410727302750:ios:950f1c0f109ffe753516cf --release-notes 'Automated build'",
    "build:android:apk": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "distribute:android": "firebase appdistribution:distribute android/app/build/outputs/apk/release/app-release.apk --app 1:410727302750:android:e763289950a648d43516cf --release-notes 'Automated Android build'",
    "android:deep-clean": "bash ./android-clean.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.876.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-firebase/app": "^21.5.0",
    "@react-native-firebase/app-distribution": "^21.5.0",
    "@react-native-firebase/messaging": "^21.5.0",
    "@react-native/gradle-plugin": "^0.80.1",
    "@react-native/new-app-screen": "0.80.1",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "i18next": "^25.7.3",
    "intl-pluralrules": "^2.0.1",
    "react": "19.1.0",
    "react-i18next": "^16.5.1",
    "react-native": "0.80.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.27.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-svg": "^15.12.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^19.1.2",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.2.7",
    "prettier": "2.8.8",
    "react-native-asset": "^2.1.1",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "bash -c 'npx tsc --noEmit'"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
